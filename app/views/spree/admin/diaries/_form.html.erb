
<%= form_for(@spree_diary, url: admin_order_diaries_path) do |form| %>


  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title , class: 'form-control mb-5' %>
  </div>

  <div class="field">
    <%= form.label :body %>
    <%= form.text_area :body %>
  </div>


  <div class="actions">
    <%= form.submit %>
  </div>
  
<% end %>
